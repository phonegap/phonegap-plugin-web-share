<?xml version="1.0" encoding="UTF-8"?>

<plugin xmlns="http://cordova.apache.org/ns/plugins/1.0"
           id="phonegap-plugin-web-share"
      version="0.0.1">
    <name>Web Share</name>

    <js-module src="www/webshare.js" name="webshare">
        <clobbers target="navigator.share" />
    </js-module>
    <dependency id="cordova-plugin-x-socialsharing"/>
    <dependency id="es6-promise-plugin"/>
</plugin>
